{
  "name": "Mine For Diamonds",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Packages": { "$path": "Packages" },
      "Shared": {
        "$path": "src/shared"
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          1,
          1,
          1
        ],
        "Brightness": 1,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}